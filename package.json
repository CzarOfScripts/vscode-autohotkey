{
	"name": "vscode-autohotkey-enhance-temp",
	"displayName": "AutoHotkey#",
	"description": "AutoHotkey language support for VS Code",
	"version": "0.1.2",
	"publisher": "helsmy",
	"engines": {
		"vscode": "^1.41.1"
	},
	"activationEvents": [
		"onLanguage:ahk"
	],
	"main": "./out/extension",
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"license": "SEE LICENSE IN LICENSE.md",
	"icon": "icon.png",
	"bugs": {
		"url": "https://github.com/helsmy/vscode-autohotkey/issues"
	},
	"homepage": "https://github.com/helsmy/vscode-autohotkey/blob/master/README.md",
	"repository": {
		"type": "git",
		"url": "https://github.com/helsmy/vscode-autohotkey.git"
	},
	"galleryBanner": {
		"color": "#5c2d91",
		"theme": "dark"
	},
	"keywords": [
		"vscode",
		"autohotkey",
		"ahk"
	],
	"contributes": {
		"languages": [
			{
				"id": "ahk",
				"aliases": [
					"AutoHotkey",
					"autohotkey",
					"ahk"
				],
				"extensions": [
					".ahk",
					".ext"
				],
				"configuration": "./ahk.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ahk",
				"scopeName": "source.ahk",
				"path": "./syntaxes/ahk.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "ahk",
				"path": "./snippets/ahk.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc --project ./",
		"watch": "tsc -watch --project ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test",
		"tslint": "tslint -t verbose src/**/*.ts"
	},
	"devDependencies": {
		"@types/node": "^8.10.58",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode": "^1.1.36"
	},
	"dependencies": {
		"n-readlines": "^1.0.0",
		"vsce": "^1.69.0"
	}
}
